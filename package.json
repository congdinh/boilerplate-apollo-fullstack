{
  "name": "apollo-app",
  "version": "1.0.0",
  "description": "Service apollo fullstack",
  "main": "index.js",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "pretest": "eslint src",
    "start:server": "node dist/index.js",
    "build:server": "NODE_ENV=production && rm -rf dist/ & babel src --out-dir dist --copy-files",
    "dev:server": "nodemon --exec babel-node src/index.js",
    "start:client": "parcel ./app/build/index.html",
    "build:client": "NODE_ENV=production && rm -rf ./app/build & parcel build ./app/public/index.html --out-dir ./app/build --target browser --no-source-maps",
    "dev:client": "parcel ./app/public/index.html --out-dir ./app/build",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "NODE_ENV=production && yarn build:server & yarn build:client",
    "dev": "NODE_ENV=development && yarn dev:server & yarn dev:client"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@elastic/elasticsearch": "^7.7.1",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "moment": "^2.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^0.14.3",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
